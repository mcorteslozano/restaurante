<script type="text/javascript">
$(document).ready(function(){
   // Click mostrar/cerrar para el mapa
   $('.show_hide').showHide({			 
		speed: 1000,  	
		easing: '', 
		changeText: 1, 
		showText: 'Mostrar',
		hideText: 'Cerrar' 		 
	}); 

});
</script>

<script>
    jQuery(document).ready(function( $ ) {
        $('.counter').counterUp({
            delay: 10,
            time: 1000
        });
    });
</script>

<script>
	// Esconder overflow hasta que no se cargue el body
	$(document).ready(function(){
	    $("body").css("overflow", "hidden");
	});
	// Mostrar overflow cuando todo esté cargado
	$(window).load(function(){
	    $("body").css("overflow", "visible");        
	    var nice=$('html').niceScroll({
		cursorborder:"5",
		cursorcolor:"#00AFF0",
		cursorwidth:"3px",
		boxzoom:true, 
		autohidemode:true
		});
	});
</script>
<!-- Final Scripts -->

